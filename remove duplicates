var removeDuplicates = function(nums) {
    let k = nums.length;
    for(let i = 0; i < nums.length; i++){
        let current = nums[i];
      	let previous = nums[i-1];
        if(current === previous){
          nums.splice(i, 1)
          i--
        }
    }
  console.log(nums)
  return nums.length
    };
removeDuplicates([0,0,1,1,1,2,2,3,3,4])



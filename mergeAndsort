var merge = function(nums1, m, nums2, n) {
 	 nums1.splice(m, n)
    for(let i = 0; i < nums2.length; i++){
      nums1.push(nums2[i])
    }
  nums1.sort((a, b) => a - b)
};


merge([1,2,3,0,0,0], 3, [2,5,6], 3)


